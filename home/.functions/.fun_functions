function funref {

MESSAGE=`cat <<HEREDOC_MESSAGE

  Aliases:
    - funbenny - opens https://www.youtube.com/watch?v=MK6TXMsvgQg.
    - funbingo - opens ~/Documents/standup-bingo.png

  Links:
    - Hacker typer - https://hackertyper.net/#

  Commands:
    - Star Wars in the Terminal - telnet towel.blinkenlights.nl

  Social games:
    - Railroad Ink - https://www.dropbox.com/s/tyvz8my2u5j7cgx/RRI_BOARD_PNP_2020_SU%26SD.pdf?dl=0
    - Just one - https://just1.herokuapp.com
    - Codenames - https://horsepaste.com
    - Wavelength - https://heretothere.app/
    - Spyfall - https://spyfall.tannerkrewson.com
    - Jackbox â€“ https://jackbox.tv

  Notes:
    - Atop adds in youtube videos by adding a period to the url - https://www.komando.com/tv-streaming/secret-trick-to-skip-ads-on-youtube/742056/

HEREDOC_MESSAGE
`
echo "$MESSAGE"
}

function funhelp {
 funref
}

alias funbenny='open https://www.youtube.com/watch?v=MK6TXMsvgQg.'
alias funbingo='open ~/Documents/standup-bingo.png'